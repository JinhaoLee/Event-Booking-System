  "dependencies": {
    "apollo-server": "^2.5.0",
    "apollo-server-express": "^2.5.0",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "dotenv": "^8.0.0",
    "express": "^4.16.4",
    "graphql": "^14.3.0",
    "jsonwebtoken": "^8.5.1",
